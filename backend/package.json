{
  "name": "chum-muffa-backend",
  "version": "1.0.0",
  "description": "CAS FEE chumm-uffa App",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node ./dist/server.js",
    "docker": "npm run build && node ./dist/server.js docker",
    "build": "npm run clean && npm run tsc && npm run html",
    "test": "npm run build && mocha -t 30000 dist/tests/**/*.spec.js --exit",
    "lint": "tslint --project tsconfig.json --exclude ./**/*.js",
    "html": "copyfiles ./**/*.html ./**/*.css ./.env dist/",
    "tsc": "tsc",
    "clean": "rimraf dist",
    "start-watch": "nodemon -e ts -w ./ -x ts-node --inspect ./src/server.ts",
    "docker-watch": "nodemon -e ts -w ./ -x ts-node --inspect ./src/server.ts docker"
  },
  "author": "daniel tellenbach & christoph kalt",
  "license": "ISC",
  "dependencies": {
    "@chumm-uffa/interface": "file:../interface",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.12.4",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.12.4",
    "express-jwt": "^5.1.0",
    "jsonwebtoken": "^7.1.9",
    "mockgoose-fix": "^7.3.6",
    "mongoose": "^4.13.6",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "rxjs": "^5.5.5",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.39",
    "@types/mocha": "^2.2.44",
    "@types/mongoose": "^4.7.28",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.0.57",
    "@types/passport": "^0.4.1",
    "@types/passport-jwt": "^3.0.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "copyfiles": "^1.2.0",
    "mocha": "^4.0.1",
    "mocha-typescript": "^1.1.12",
    "nodemon": "^1.12.5",
    "rename": "^1.0.4",
    "rimraf": "^2.6.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}
