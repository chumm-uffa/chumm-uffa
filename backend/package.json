{
  "name": "chum-muffa",
  "version": "1.0.0",
  "description": "CAS FEE chumm-uffa App",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node ./dist/server.js",
    "build": "npm run clean && npm run tsc && npm run html",
    "dev": "npm run clean && npm run watch",
    "lint": "npm run clean && tslint --project tsconfig.json --exclude ./**/*.js",
    "html": "copyfiles ./**/*.html ./**/*.css dist/",
    "tsc": "tsc",
    "clean": "rimraf dist",
    "watch": "nodemon -e ts -w ./ -x ts-node --inspect main.ts"
  },
  "author": "daniel tellenbach & christoph kalt",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.12.4",
    "express": "^4.12.4",
    "express-jwt": "^5.1.0",
    "jsonwebtoken": "^7.1.9",
    "nedb": "^1.8.0"
  },
  "devDependencies": {
    "copyfiles": "^1.2.0",
    "nodemon": "^1.12.5",
    "rimraf": "^2.6.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}
